{
	"name": "server",
	"version": "1.0.0",
	"type": "module",
	"private": true,
	"engines": {
		"node": "20.0.0"
	},
	"scripts": {
		"dev": "pnpm run build && concurrently \"pnpm run watch-compile\" \"pnpm run watch-dev\"",
		"dev:static": "NODE_ENV=dev pnpm run build && concurrently \"pnpm run watch-compile\" \"pnpm run watch-dev\"",
		"watch-compile": "swc src -w --out-dir dist",
		"watch-dev": "nodemon --watch \"dist/**/*\" -e js ./dist/e2e-server.js",
		"build": "rm -rf dist && swc src --out-dir dist",
		"start": "node dist/app.js",
		"start:e2e": "pnpm run build && node dist/e2e-testing-server.js",
		"start:production": "node dist/app.js",
		"lint": "npx @biomejs/biome check ./src",
		"lint:fix": "npx @biomejs/biome check --write --unsafe ./src"
	},
	"dependencies": {
		"cors": "^2.8.5",
		"express": "^4.18.2",
		"express-rate-limit": "^6.7.0",
		"mongoose": "^7.0.4",
		"ws": "^8.13.0"
	},
	"devDependencies": {
		"@swc/cli": "^0.1.62",
		"@swc/jest": "^0.2.26",
		"@types/cors": "^2.8.13",
		"@types/express": "^4.17.17",
		"@types/morgan": "^1.9.4",
		"@types/node": "^18.15.3",
		"@types/supertest": "^2.0.12",
		"@types/ws": "^8.5.4",
		"chokidar": "latest",
		"dotenv": "^16.0.3",
		"mongodb-memory-server": "10.0.0",
		"morgan": "^1.10.0",
		"nodemon": "latest"
	}
}
